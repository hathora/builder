import { Methods, Context } from "./.hathora/methods";
import { Response } from "./.hathora/base";
import {
{{#each types}}
  {{@key}},
{{/each}}
{{#each methods}}
  {{makeRequestName @key}},
{{/each}}
} from "./.hathora/types";

type InternalState = {};

export class Impl implements Methods<InternalState> {
  {{initialize}}(userId: UserId, ctx: Context, request: {{makeRequestName initialize}}): InternalState {
    return {};
  }
  {{#each methods}}
  {{#if (ne @key ../initialize)}}
  {{@key}}(state: InternalState, userId: UserId, ctx: Context, request: {{makeRequestName @key}}): Response {
    return Response.error("Not implemented");
  }
  {{/if}}
  {{/each}}
  getUserState(state: InternalState, userId: UserId): {{userState}} {
    return {
      {{#with (lookup types userState)}}
      {{#each properties}}
      {{@key}}: {{> renderDefault}},
      {{/each}}
      {{/with}}
    };
  }
  {{#if tick}}
  onTick(state: InternalState, ctx: Context, timeDelta: number): void {}
  {{/if}}
}
{{#*inline "renderDefault"}}
{{#if (eq type "array")}}
[]
{{~else if (eq type "int")}}
0
{{~else if (eq type "float")}}
0.0
{{~else if (eq type "string")}}
""
{{~else if (eq type "enum")}}
0
{{~else if (eq type "boolean")}}
false
{{~else if (eq type "optional")}}
undefined
{{~else if (eq type "object")}}
{{typeString}}.default()
{{~else if (eq type "union")}}
{{#each options}}
{{#if @first}}
{ type: "{{@key}}", val: {{> renderDefault}} }
{{~/if}}
{{/each}}
{{~else if (eq type "plugin")}}
{{> renderDefault item}}
{{/if}}
{{/inline}}
