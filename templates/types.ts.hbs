{{#each types}}
{{#with (getArgsInfo this)}}
{{#if (eq type "enum")}}
export enum {{@key}} {
{{#each options}}
  {{label}},
{{/each}}
}
{{else if (eq type "object")}}
export interface {{@key}} {
{{#each properties}}
  {{@key}}{{#unless required}}?{{/unless}}: {{typeString}};
{{/each}}
}
{{else}}
export type {{@key}} = {{../this}};
{{/if}}
{{/with}}
{{/each}}
{{#each methods}}
export interface {{makeRequestName @key}} {
{{#join this ";
" false true}}  {{name}}: {{type}}{{/join~}}
}
{{/each}}
